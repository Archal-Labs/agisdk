{
  "id": "gocalendar-gomail-zilloft-1",
  "goal": "Find the two cheapest 3-bedroom rental homes in Austin, TX on Zilloft. Then, compose an email to myself at my.personal.email@gomail.com with the subject 'Rental Comparison'. In the body, list both property addresses and their monthly prices. Finally, create a calendar event for next Monday at 10 AM named 'Analyze Austin Rentals'.",
  "websites": [
    {
      "id": "gocalendar",
      "name": "GoCalendar",
      "similarTo": "Google Calendar",
      "previewImage": "/websitePreviews/gocalendar_preview.jpg",
      "url": "https://real-gocalendar.vercel.app"
    },
    {
      "id": "gomail",
      "name": "GoMail",
      "similarTo": "Gmail",
      "previewImage": "/websitePreviews/gomail_preview.jpg",
      "url": "https://real-gomail.vercel.app"
    },
    {
      "id": "zilloft",
      "name": "Zilloft",
      "similarTo": "Zillow",
      "previewImage": "/websitePreviews/zilloft_preview.jpg",
      "url": "https://real-zilloft.vercel.app"
    }
  ],
  "difficulty": "hard",
  "challengeType": "action",
  "possible": true,
  "evals": [
    {
      "description": "Verify an email was sent to the correct recipient ('my.personal.email@gomail.com') with the subject 'Rental Comparison'.",
      "type": "jmespath",
      "query": "length(gomail.differences.emails.added[?sent == `true`]) >= `1` && contains(gomail.differences.emails.added[?sent == `true`][0].recipient, 'my.personal.email@gomail.com') && contains(gomail.differences.emails.added[?sent == `true`][0].subject, 'Rental Comparison')",
      "expected_value": true
    },
    {
      "description": "Verify the email body contains text indicating at least two rental properties (e.g., 'Address:' and 'Price:'), implying the Zilloft data was included.",
      "type": "jmespath",
      "query": "contains(gomail.differences.emails.added[?sent == `true`][0].content, 'Address:') && contains(gomail.differences.emails.added[?sent == `true`][0].content, 'Price:')",
      "expected_value": true
    },
    {
      "description": "Verify a calendar event named 'Analyze Austin Rentals' was created and scheduled for 10 AM.",
      "type": "jmespath",
      "query": "length(gocalendar.differences.events.added) >= `1` && contains(gocalendar.differences.events.added.values(@)[0].title, 'Analyze Austin Rentals') && contains(gocalendar.differences.events.added.values(@)[0].start.dateTime, '10:00:00')",
      "expected_value": true
    }
  ],
  "points": 2,
  "config": {}
}