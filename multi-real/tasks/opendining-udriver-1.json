      "description": "Verify a table reservation was made on OpenDining.",
      "type": "jmespath",
      "query": "length(opendining.differences.bookings.added) >= `1`",
      "expected_value": true
    },
    {
      "description": "Verify the OpenDining reservation has the correct party size and time.",
      "type": "jmespath",
      "query": "opendining.differences.bookings.added[0].partySize == `3` && contains(opendining.differences.bookings.added[0].date, '20:30')",
      "expected_value": false
    }
