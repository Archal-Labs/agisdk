{
  "id": "gocalendar-topwork-2",
  "goal": "Search for 'entry-level frontend developer' candidates in Boston on TopWork. Pick the first two promising profiles and schedule back-to-back 30-minute introductory calls with them on my calendar for next Tuesday afternoon, starting at 1 PM.",
  "websites": [
    {
      "id": "gocalendar",
      "name": "GoCalendar",
      "similarTo": "Google Calendar",
      "previewImage": "/websitePreviews/gocalendar_preview.jpg",
      "url": "https://real-gocalendar.vercel.app"
    },
    {
      "id": "topwork",
      "name": "TopWork",
      "similarTo": "Indeed",
      "previewImage": "/websitePreviews/topwork_preview.jpg",
      "url": "https://real-topwork.vercel.app"
    }
  ],
  "difficulty": "hard",
  "challengeType": "action",
  "possible": true,
  "evals": [
    {
      "description": "Verify that two calendar events were created for the introductory calls.",
      "type": "jmespath",
      "query": "length(gocalendar.differences.events.added)",
      "expected_value": true
    },
    {
      "description": "Verify the title of the first scheduled introductory call event.",
      "type": "jmespath",
      "query": "contains(gocalendar.differences.events.added.values(@)[0].title, 'Introductory Call')",
      "expected_value": true
    },
    {
      "description": "Verify the title of the second scheduled introductory call event.",
      "type": "jmespath",
      "query": "contains(gocalendar.differences.events.added.values(@)[1].title, 'Introductory Call')",
      "expected_value": true
    },
    {
      "description": "Verify the start time of the first introductory call is scheduled for 1 PM.",
      "type": "jmespath",
      "query": "contains(gocalendar.differences.events.added.values(@)[0].start.dateTime, '13:00:00')",
      "expected_value": true
    }
  ],
  "points": 2,
  "config": {}
}