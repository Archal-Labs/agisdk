{
  "id": "gocalendar-staynb-2",
  "goal": "I need to plan a work retreat for my team in Miami, Florida for late January. Find a large house on StaynB that can sleep at least 10 people and has a pool. Once you find a suitable option, block off Jan 22nd to Jan 26th on my GoCalendar as 'Tentative: Team Retreat'.",
  "websites": [
    {
      "id": "gocalendar",
      "name": "GoCalendar",
      "similarTo": "Google Calendar",
      "previewImage": "/websitePreviews/gocalendar_preview.jpg",
      "url": "https://real-gocalendar.vercel.app"
    },
    {
      "id": "staynb",
      "name": "StaynB",
      "similarTo": "Airbnb",
      "previewImage": "/websitePreviews/staynb_preview.jpg",
      "url": "https://real-staynb.vercel.app"
    }
  ],
  "difficulty": "hard",
  "challengeType": "action",
  "possible": true,
  "evals": [
    {
      "description": "Verify that a booking was added in StaynB for the team retreat.",
      "type": "jmespath",
      "query": "length(values(staynb.initialfinaldiff.added.booking.bookingDetails || `{}`)) >= `1`",
      "expected_value": true
    },
    {
      "description": "Verify that the booked property in StaynB is located in Miami (checking name for keyword).",
      "type": "jmespath",
      "query": "contains(values(staynb.initialfinaldiff.added.booking.bookingDetails)[0].hotelName, 'Miami') || contains(values(staynb.initialfinaldiff.added.booking.bookingDetails)[0].location, 'Miami')",
      "expected_value": true
    },
    {
      "description": "Verify that a calendar event titled 'Tentative: Team Retreat' was added to GoCalendar.",
      "type": "jmespath",
      "query": "length(gocalendar.differences.events.added) >= `1` && contains(gocalendar.differences.events.added.values(@)[0].title, 'Tentative: Team Retreat')",
      "expected_value": true
    },
    {
      "description": "Verify that the GoCalendar event for the retreat is scheduled from January 22nd to January 26th.",
      "type": "jmespath",
      "query": "gocalendar.differences.events.added.values(@)[0].start.date == '2025-01-22' && gocalendar.differences.events.added.values(@)[0].end.date == '2025-01-26'",
      "expected_value": true
    }
  ],
  "points": 2,
  "config": {}
}